<!DOCTYPE html>
<html>

<head>
  <% include ../../partials/header.ejs %>
</head>

<body>
  <div class="wrapper ">
    <% include ../../partials/sidebar.ejs %>

    <div class="main-panel ps-container ps-theme-default ps-active-y">
      <!-- Navbar -->
      <% include ../../partials/navbar.ejs %>
      <!-- End Navbar -->
      <div class="content">
        <div class="row">
          <div class="col-lg-12 col-md-12">
            <button class="btn btn-primary" data-toggle="modal" data-target="#noticeModal">Create Group</button>
          </div>
          <div class="col-lg-12 col-md-12">
            <div class="row">
              <% results.forEach(function(r) { %>
              <div class="server card" data-id="<%= r._id %>">

                <input class="bootstrap-switch" type="checkbox" data-toggle="switch" checked data-on-color="default"
                  data-off-color="default" data-on-label="Main" data-off-label="Sycn">
              </div>
              <% }); %>
            </div>
          </div>
        </div>
      </div>
      <% include ../../partials/footer.ejs %>
    </div>
  </div>
  <% include ../../partials/sidebar-background.ejs %>
  <% include ../../partials/footer-js-core.ejs %>
  <%- include('../../partials/modals/notice-modal.ejs', {modaltype: 'noticeModal', contenttype: 'createGroup'}) %>

  <script src="/assets/js/enums.js" defer></script>
  <script src="/assets/js/module.js" defer></script>
  <script src="/assets/js/group/group.js" defer></script>

</body>

</html>